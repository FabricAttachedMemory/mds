# Directories to search for config files
# cmd_line.config_file_path: [., /home/evank]

# Suffix for configuration files
# cmd_line.config_file_suffix: ".config"

# What to dump with -dump_params
# cmd_line.dump: [PARAMS, DEFAULTS, HELP, SOURCES]

# Default width for lines with -dump_params
# cmd_line.dump_line_width: 80

# Dump param values
# cmd_line.dump_params: true

# File to dump params to with -dump_params or null to dump to terminal
# cmd_line.dump_params_to: shop.config

# Show usage help and help on available params
# cmd_line.help: true

# Default width for lines with -help
# cmd_line.help_line_width: 100

# What to give help on with -help
# cmd_line.help_on: [PARAMS, DEFAULTS, RESTRICTIONS, TYPES, ONLY, FUNCTIONS,
#                   SUBTYPES, SYNTAX, EMPTY, CONVERSIONS]

# Exit after processing params
# cmd_line.stop_after_parser: true
