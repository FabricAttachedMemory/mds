# The number of milliseconds to block
# shop.blockms: 1_000

# The name of the inventory in the namespace
# shop.inventory: "Inventory3"

# The maximum number of retries of a transaction
# shop.max_retries: 3

# The maximum quantity of an order
# shop.maxq: 20

# The mean delay (in ms) between transactions on a thread
# shop.mean_delay_ms: 100

# The minimum quantity of an order
# shop.minq: 1

# Products less than this number will be considered 'hot'
# shop.nhot: 3

# The number of parallel threads to launch
# shop.nthreads: 5

# The probability of a block in a transaction
# shop.pblock: 0.1

# Percentage of transactions that go to 'hot' products
# shop.phot: 0.1

# The quantity of a restock
# shop.restock: 100

# The standard deviation of delay (in ms) between transactions on a thread
# shop.sd_delay_ms: 10

# The name of the shop
# shop.shop: "shop1"

# The maximum run length, in seconds
shop.stop_after: 12000
